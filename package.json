{
  "name": "@selfaware/wool",
  "version": "0.1.3",
  "description": "A comprehensive and flexible utility-first styling solution.",
  "main": "index.js",
  "scripts": {
    "build": "sass --style=expanded src/index.scss dist/wool.css;",
    "watch": "sass --watch --style=expanded src/index.scss dist/wool.css;",
    "cssnano": "postcss dist/wool.css > dist/wool.min.css"
  },
  "keywords": ["sass", "scss", "functional", "utility-first", "style", "styles", "styling"],
  "homepage": "https://github.com/selfawarestudio/wool",
  "repository": "selfawarestudio/wool",
  "author": "Self Aware <howdy@selfaware.studio> (https://selfaware.studio)",
  "license": "MIT",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "postcss-cli": "^6.1.2",
    "sass": "^1.23.1"
  }
}
