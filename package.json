{
  "name": "@selfaware/wool",
  "version": "0.0.1",
  "description": "CSS utility classes for Self Aware projects",
  "main": "index.js",
  "scripts": {
    "build": "node-sass --output-style expanded --source-map dist/wool.css.map src/index.scss dist/wool.css;",
    "watch": "node-sass -w --output-style expanded --source-map dist/wool.css.map src/index.scss dist/wool.css;",
    "cssnano": "postcss dist/wool.css > dist/wool.min.css"
  },
  "keywords": [],
  "author": "Self Aware <howdy@selfaware.studio> (https://selfaware.studio)",
  "license": "MIT",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2"
  }
}
